{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--read-only",
        "--project-ref=jjpklpivpvywagmjjwpu"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      },
      "autoApprove": [
        "supabase_db_query",
        "supabase_db_execute",
        "supabase_list_tables",
        "supabase_describe_table",
        "supabase_db_list_tables",
        "supabase_db_describe_table",
        "supabase_db_query_sql",
        "supabase_db_execute_sql"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "ghcr.io/github/github-mcp-server:latest"
      ],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "autoApprove": [
        "push_files",
        "update_file",
        "create_issue",
        "get_file",
        "list_files",
        "search_code",
        "list_commits",
        "get_commit"
      ]
    }
  }
}
