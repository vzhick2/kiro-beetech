{
  "name": "btinv",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "next dev --turbo --port 3002",
    "build": "next build",
    "build:time": "echo 'Build started at %time%' && next build && echo 'Build completed at %time%'",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "ai:validate": "pnpm ai:type-check && pnpm lint && pnpm format --check",
    "ai:validate:full": "pnpm type-check && pnpm lint && pnpm format --check",
    "ai:type-check": "tsc --noEmit --skipLibCheck",
    "ai:fix": "pnpm lint --fix && pnpm format --write",
    "ai:commit": "pnpm ai:validate && git add . && git commit",
    "commit:fast": "git add . && git commit --no-verify -m",
    "commit:safe": "git add . && git commit -m",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy:auto": "git add . && git commit -m \"Auto deployment\" && git push origin main",
    "deploy:msg": "echo 'Usage: npm run deploy:msg -- \"Your commit message\"' && git add . && git commit -m",
    "supabase:types": "npx supabase gen types typescript --project-id jjpklpivpvywagmjjwpu > src/types/database.ts",
    "supabase:link": "echo 'Use: npx supabase link --project-ref jjpklpivpvywagmjjwpu -p YOUR_PASSWORD'",
    "supabase:status": "npx supabase status",
    "supabase:api-keys": "npx supabase projects api-keys --project-ref jjpklpivpvywagmjjwpu",
    "supabase:db-pull": "echo 'Use: npx supabase db pull --project-ref jjpklpivpvywagmjjwpu -p YOUR_PASSWORD'",
    "supabase:db-push": "echo 'Use: npx supabase db push --project-ref jjpklpivpvywagmjjwpu -p YOUR_PASSWORD'"
  },
